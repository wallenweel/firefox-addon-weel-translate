{
  "title": "WeeL Translate",
  "name": "weel-translate",
  "version": "0.1.5",
  "description": "An Easy Well Translation",
  "author": "wallen",
  "main": "index.js",
  "permissions": {
      "multiprocess": true
  },
  "preferences": [{
      "name": "mainPanelWidth",
      "title": "Main Panel Width",
      "type": "integer",
      "value": 300,
      "hidden": true
      }, {
      "name": "floatPanelWidth",
      "title": "Float Panel Width",
      "type": "integer",
      "value": 200,
      "hidden": true
      }, {
      "name": "select2show",
      "title": "交互模式",
      "description": "更改模式后需刷新已经载入的标签页来使其生效",
      "type": "radio",
      "value": 3,
      "options": [
          {
              "value": 0,
              "label": "只从按钮启动"
          }, {
              "value": 1,
              "label": "自动出现在按钮位置"
          }, {
              "value": 2,
              "label": "自动出现在选中文字位置（实验性）"
          }, {
              "value": 3,
              "label": "显示浮动按钮"
          }
      ]
  }, {
      "name": "apiID",
      "title": "API ID",
      "description": "从有道申请的“keyfrom”值",
      "type": "string",
      "value": ""
  }, {
      "name": "apiKey",
      "title": "API Key",
      "description": "从有道申请的“key”值",
      "type": "string",
      "value": ""
  }],
  "engines": {
    "firefox": ">=45.0"
  },
  "license": "MIT",
  "keywords": [
    "weel",
    "translate",
    "translation",
    "dictionary"
  ]
}
